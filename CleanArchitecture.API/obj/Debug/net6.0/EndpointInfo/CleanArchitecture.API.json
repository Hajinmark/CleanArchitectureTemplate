{
  "openapi": "3.0.1",
  "info": {
    "title": "CleanArchitecture.API",
    "version": "1.0"
  },
  "paths": {
    "/api/Movies/FetchMoviesData": {
      "get": {
        "tags": [
          "Movies"
        ],
        "responses": {
          "200": {
            "description": "Success"
          }
        }
      }
    },
    "/api/Movies/AllMovies": {
      "get": {
        "tags": [
          "Movies"
        ],
        "responses": {
          "200": {
            "description": "Success"
          }
        }
      }
    },
    "/api/Movies/AddMovie": {
      "post": {
        "tags": [
          "Movies"
        ],
        "requestBody": {
          "content": {
            "application/json": {
              "schema": {
                "$ref": "#/components/schemas/MovieDTO"
              }
            },
            "text/json": {
              "schema": {
                "$ref": "#/components/schemas/MovieDTO"
              }
            },
            "application/*+json": {
              "schema": {
                "$ref": "#/components/schemas/MovieDTO"
              }
            }
          }
        },
        "responses": {
          "200": {
            "description": "Success"
          }
        }
      }
    }
  },
  "components": {
    "schemas": {
      "MovieDTO": {
        "type": "object",
        "properties": {
          "name": {
            "type": "string",
            "nullable": true
          },
          "cost": {
            "type": "number",
            "format": "double",
            "nullable": true
          }
        },
        "additionalProperties": false
      }
    }
  }
}